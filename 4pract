    print(True)
else:
    print(False)

#2.3
r = int(input())
g = int(input())
b = int(input())

if r == 0 and g == 0 and b == 0:
    print("–ß—ë—Ä–Ω—ã–π —Ü–≤–µ—Ç")
elif r == 255 and g == 255 and b == 255:
    print("–ë–µ–ª—ã–π —Ü–≤–µ—Ç")
elif r == 255 and g == 0 and b == 0:
    print("–ö—Ä–∞—Å–Ω—ã–π —Ü–≤–µ—Ç")
elif r == 0 and g == 255 and b == 0:
    print("–ó–µ–ª—ë–Ω—ã–π —Ü–≤–µ—Ç")
elif r == 0 and g == 0 and b == 255:
    print("–°–∏–Ω–∏–π —Ü–≤–µ—Ç")
else:
    print("–ù–µ—Ç —Ü–≤–µ—Ç–∞")

#3.1
a = int(input())

if a > 0:
    print(a - 1, a, a + 1)
else:
    a = 1
    print(a - 1, a, a + 1)

#3.2
a = input()

if a.endswith('.doc'):
    print("Word file")
elif a.endswith('.py'):
    print("Python file")
elif a.endswith('.txt'):
    print("Text file")
else:
    print("Unknown extension")

#3.3
a = float(input())
b = float(input())
c = float(input())

if a == b == c:
    print("–†–∞–≤–Ω–æ—Å—Ç–æ—Ä–æ–Ω–Ω–∏–π")
elif a == b or b == c or a == c:
    print("–†–∞–≤–Ω–æ–±–µ–¥—Ä–µ–Ω–Ω—ã–π")
else:
    print("–†–∞–∑–Ω–æ—Å—Ç–æ—Ä–æ–Ω–Ω–∏–π")

