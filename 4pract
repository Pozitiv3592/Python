#11.1
prog_num = 0
a = int(input())
b = int(input())

if a < 0 and b < 0:
    prog_num = a + b
    print(prog_num)
elif a > 0 and b > 0:
    prog_num = a - b
    print(prog_num)
else:
    print(False)

#11.2
