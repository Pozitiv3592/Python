if a < 0:
    a = abs(a)
elif a == 0:
    a = 1

print(a)
#1.2
a = input()
b = False

for i in a:
    if i == '.' or i == ',':
        b = True
        break
else:
    b = False

print(b)
#1.3
a = int(input())
b = int(input())

if a % 3 == 0 and b % 3 == 0:
    print(True)
elif a % 3 == 0 or b % 3 == 0:
    print("–û–¥–Ω–æ —á–∏—Å–ª–æ –¥–µ–ª–∏—Ç—Å—è –Ω–∞ 3")
else:
    print(False)

#2.1
n = int(input())

if n > 100:
    print('*')
elif n > 0:
    for i in range(n):
        print('*', end='')
    print()

#2.2
